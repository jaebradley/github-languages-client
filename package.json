{
  "name": "github-languages-client",
  "version": "0.0.0-development",
  "description": "NodeJS Client to get languages used on GitHub",
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "browser": "build/index.js",
  "files": [
    "build"
  ],
  "global": false,
  "scripts": {
    "build": "BABEL_ENV=production rollup -c",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "is-es5": "es-check es5 build/**/index.cjs.js build/**/index.js",
    "lint": "eslint --ext .js .",
    "test": "BABEL_ENV=test jest src/ --coverage --no-cache",
    "prepare": "npm run build",
    "gc": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "github",
    "programming languages",
    "programming language",
    "github programming languages",
    "github languages",
    "github language"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/github-languages-client"
  },
  "bugs": {
    "url": "https://github.com/jaebradley/github-languages-client/issues"
  },
  "homepage": "https://github.com/jaebradley/github-languages-client/#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "codecov": "^3.0.2",
    "es-check": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^1.0.0",
    "jest": "^22.4.3",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-babel-minify": "^6.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^5.0.0"
  },
  "dependencies": {
    "fuse.js": "^3.2.0"
  }
}
