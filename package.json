{
  "name": "github-languages-client",
  "version": "0.0.0-development",
  "description": "NodeJS Client to get languages used on GitHub",
  "author": "jae.b.bradley@gmail.com",
  "main": "build/index.js",
  "global": false,
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "babel src/ -d build/ --delete-dir-on-start --copy-files",
    "compile:with-scripts": "babel src/ scripts -d build/ --delete-dir-on-start",
    "compile:prod": "BABEL_ENV=production npm run compile",
    "compile:dev": "BABEL_ENV=development npm run compile:with-scripts",
    "is-es5": "es-check es5 build/*.js",
    "lint": "eslint --ext .js .",
    "test": "jest src/ --coverage --no-cache",
    "prepublishOnly": "npm run compile:prod",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [
    "github",
    "programming languages",
    "programming language",
    "github programming languages",
    "github languages",
    "github language"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/github-languages-client"
  },
  "bugs": {
    "url": "https://github.com/jaebradley/github-languages-client/issues"
  },
  "homepage": "https://github.com/jaebradley/github-languages-client/#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/prompt": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "camelize": "^1.0.0",
    "codecov": "^3.0.0",
    "es-check": "^2.0.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "semantic-release": "^15.1.5",
    "travis-deploy-once": "^4.4.1",
    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "fuse.js": "^3.2.0"
  }
}
